{
  "version": "3.0",
  "id": "default-scenario",
  "name": "Default Scenario",
  "description": "A simple working scenario with source, process, and sink nodes",
  "nodes": [
    {
      "nodeId": "source1",
      "type": "DataSource",
      "displayName": "Token Source",
      "position": { "x": 100, "y": 150 },
      "interval": 3,
      "generation": {
        "type": "random",
        "valueMin": 1,
        "valueMax": 10
      },
      "outputs": [
        {
          "destinationNodeId": "process1"
        }
      ]
    },
    {
      "nodeId": "process1",
      "type": "ProcessNode",
      "displayName": "Processor",
      "position": { "x": 350, "y": 150 },
      "inputs": [
        {
          "nodeId": "source1"
        }
      ],
      "outputs": [
        {
          "destinationNodeId": "sink1"
        }
      ],
      "formulas": [
        "input1.value * 2"
      ]
    },
    {
      "nodeId": "sink1",
      "type": "Sink",
      "displayName": "Output Sink",
      "position": { "x": 600, "y": 150 },
      "inputs": [
        {
          "nodeId": "process1"
        }
      ],
      "logLevel": "info",
      "maxTokens": 1000
    }
  ]
}
